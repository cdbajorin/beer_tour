<%= form_for user do |f| %>
  <%= f.text_field :username, {placeholder: "username"} %>
  <%= f.text_field :streetAddress, {placeholder: "915 Broadway"} %>
  <%= f.text_field :city, {placeholder: "New York"} %>
  <%= f.text_field :state, {placeholder: "New York"} %>
  <%= f.text_field :zipcode, {placeholder: "10010"} %>
  <%= f.text_field :bio, {placeholder: "bio"} %>
  <%= f.submit %>
<% end %>
