<%= form_for user_local do |f| %>
  <%= f.text_field :username, {placeholder: "username"} %>
  <%= f.text_field :street_address, {placeholder: "915 Broadway"} %>
  <%= f.text_field :city, {placeholder: "New York"} %>
  <%= f.text_field :state, {placeholder: "New York"} %>
  <%= f.text_field :zipcode, {placeholder: "10028"} %>
  <%= f.text_field :bio, {placeholder: "bio"} %>
  <%= f.submit %>
<% end %>
